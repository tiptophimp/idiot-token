{
  "name": "idiot-liquidity-migration",
  "version": "1.0.0",
  "description": "Liquidity migration scripts for IDIOT Token from V2 to V3",
  "main": "index.js",
  "scripts": {
    "remove-v2": "npx hardhat run removeV2Liquidity.js --network base",
    "add-v3": "npx hardhat run addV3Liquidity.js --network base",
    "migrate": "npm run remove-v2 && npm run add-v3"
  },
  "dependencies": {
    "@openzeppelin/contracts": "^4.9.0",
    "@uniswap/v2-core": "^1.0.1",
    "@uniswap/v2-periphery": "^1.1.0-beta.0",
    "@uniswap/v3-core": "^1.0.1",
    "@uniswap/v3-periphery": "^1.4.4",
    "hardhat": "^2.17.0"
  },
  "devDependencies": {
    "@nomicfoundation/hardhat-toolbox": "^3.0.0"
  }
}
